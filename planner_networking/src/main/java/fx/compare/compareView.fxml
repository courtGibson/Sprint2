<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.compare.CompareViewController">
   <center>
      <BorderPane onMouseMoved="#buildTree" prefHeight="529.0" prefWidth="675.0" style="-fx-background-color: lightgrey;" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Text fx:id="planA" strokeType="OUTSIDE" strokeWidth="0.0" text="Plan Year" translateX="100.0" translateY="80.0">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Text>
                  <Text fx:id="comparePlans" strokeType="OUTSIDE" strokeWidth="0.0" text="%comparePlan.text" translateX="130.0" translateY="30.0">
                     <font>
                        <Font size="26.0" />
                     </font>
                  </Text>
                  <VBox prefHeight="100.0" prefWidth="226.0" translateX="30.0">
                     <children>
                        <HBox prefHeight="35.0" prefWidth="226.0">
                           <children>
                              <Button fx:id="homePage" mnemonicParsing="false" onAction="#homepage" prefHeight="26.0" prefWidth="101.0" text="%homePage.text" translateY="5.0" />
                              <Button fx:id="logout" mnemonicParsing="false" onAction="#logout" text="%logout.text" translateX="60.0" translateY="5.0" />
                           </children>
                        </HBox>
                        <Text fx:id="planB" strokeType="OUTSIDE" strokeWidth="0.0" text="Plan Year" translateX="120.0" translateY="45.0">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Text>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </top>
         <left>
            <TreeView fx:id="tree1" prefHeight="429.0" prefWidth="323.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <TreeView fx:id="tree2" prefHeight="429.0" prefWidth="319.0" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </center>
</BorderPane>

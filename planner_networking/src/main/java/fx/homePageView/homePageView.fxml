<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="565.0" prefWidth="765.0" xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.homePageView.HomePageViewController">
   <top>
      <HBox prefHeight="65.0" prefWidth="738.0" BorderPane.alignment="CENTER">
         <children>
            <HBox prefHeight="66.0" prefWidth="201.0" style="-fx-background-color: lightgrey;">
               <children>
                  <Label fx:id="userText" prefHeight="22.0" prefWidth="108.0" text="%username.text" translateX="10.0" translateY="20.0" underline="true">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="user" translateX="10.0" translateY="20.0">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="66.0" prefWidth="268.0" style="-fx-background-color: lightgrey;">
               <children>
                  <Label fx:id="deptText" contentDisplay="CENTER" prefHeight="22.0" prefWidth="121.0" text="%department.text" translateY="20.0" underline="true">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="dept" translateY="20.0">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="66.0" prefWidth="193.0" style="-fx-background-color: lightgrey;" />
            <HBox prefHeight="66.0" prefWidth="157.0" style="-fx-background-color: lightgrey;">
               <children>
                  <Button id="logoutButton" fx:id="logoutButton" mnemonicParsing="false" onAction="#logout" text="%logout.text" translateX="30.0" translateY="20.0" />
               </children>
            </HBox>
         </children>
      </HBox>
   </top>
   <center>
      <Pane prefHeight="421.0" prefWidth="549.0" style="-fx-background-color: silver;" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="homePage" layoutX="126.0" layoutY="23.0" text="%homePage.text" underline="true">
               <font>
                  <Font size="35.0" />
               </font>
            </Label>
            <Button id="submit" fx:id="planSubmitButton" layoutX="209.0" layoutY="290.0" mnemonicParsing="false" onAction="#submit" text="%submitSelection.text" />
            <Label fx:id="deptPlans" layoutX="202.0" layoutY="141.0" text="%deptPlans.text">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>

            <ComboBox id="menu" fx:id="menu" layoutX="186.0" layoutY="183.0" onAction="#selectPlan" onMouseClicked="#makeMenu" prefHeight="25.0" prefWidth="173.0" promptText="%selectPlan.text" />

         </children>
      </Pane>
   </center>
   <right>
      <Pane prefHeight="446.0" prefWidth="103.0" BorderPane.alignment="CENTER" />
   </right>
   <left>
      <Pane prefHeight="500.0" prefWidth="113.0" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <Pane prefHeight="54.0" prefWidth="766.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.choosePlan.ChoosePlanController">
   <center>
      <Pane prefHeight="306.0" prefWidth="454.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane layoutX="60.0" layoutY="-11.0" prefHeight="240.0" prefWidth="298.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="271.0" minWidth="10.0" prefWidth="242.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="125.39996337890625" minWidth="10.0" prefWidth="56.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="132.00000610351563" minHeight="10.0" prefHeight="73.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="164.0" minHeight="10.0" prefHeight="58.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="145.0" minHeight="10.0" prefHeight="52.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="145.0" minHeight="10.0" prefHeight="66.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label id="viewPlanLabel" fx:id="viewPlanLabel" prefHeight="18.0" prefWidth="194.0" text="%viewPlan.text" textAlignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label id="newPlanLabel" fx:id="newPlanLabel" prefHeight="56.0" prefWidth="232.0" text="%makeNewPlan.text" textAlignment="CENTER" wrapText="true" GridPane.rowIndex="1">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="2">
                     <children>
                        <Label id="planYearLabel" fx:id="planYearLabel" prefHeight="18.0" prefWidth="148.0" text="%newPlanYear.text">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>
                        <TextField id="newPlanYearText" fx:id="newPlanYearText" prefHeight="26.0" prefWidth="73.0">
                           <HBox.margin>
                              <Insets right="20.0" />
                           </HBox.margin>
                        </TextField>
                     </children>
                  </HBox>
                  <RadioButton id="viewPlanRBtn" fx:id="viewPlanRBtn" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="18.0" prefWidth="38.0" translateX="10.0" GridPane.columnIndex="1" />
                  <RadioButton id="newPlanRBtn" fx:id="newPlanRBtn" mnemonicParsing="false" prefHeight="18.0" prefWidth="46.0" translateX="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label GridPane.rowIndex="3" />
                  <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3">
                     <children>
                        <Label fx:id="compare" prefHeight="21.0" prefWidth="156.0" text="%compareTo.text" />
                        <ComboBox fx:id="compareMenu" onAction="#selectPlan" onMouseClicked="#makeMenu" prefHeight="31.0" prefWidth="132.0" promptText="%selectComparePlan.text" />
                     </children>
                  </HBox>
                  <RadioButton fx:id="compareBtn" mnemonicParsing="false" translateX="10.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
            <Button fx:id="planSubBtn" layoutX="188.0" layoutY="231.0" mnemonicParsing="false" onAction="#choosePlanType" prefHeight="36.0" prefWidth="78.0" text="%submit.text" />
         </children>
      </Pane>
   </center>
   <top>
      <HBox prefHeight="106.0" prefWidth="454.0" style="-fx-background-color: lightgrey;" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="selectedPlan" prefHeight="27.0" prefWidth="210.0" text="%selectedPlan.text" translateX="175.0" translateY="40.0" underline="true">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label id="selectedPlanName" fx:id="planName" opacity="0.0" text="plan name" translateX="120.0" translateY="40.0">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </top>
</BorderPane>
